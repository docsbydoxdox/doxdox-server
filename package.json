{
  "name": "@neogeek/doxdox-server",
  "version": "0.0.1",
  "description": "On demand documentation generated by doxdox.",
  "engines": {
    "node": ">=7.0"
  },
  "main": "web.js",
  "license": "MIT",
  "devDependencies": {
    "@neogeek/eslint-config-standards": "1.10.0",
    "eslint": "3.12.2"
  },
  "dependencies": {
    "doxdox": "2.0.1",
    "express": "4.14.0",
    "express-enrouten": "1.3.0",
    "express-hbs": "1.0.3",
    "express-session": "1.14.2",
    "jszip": "3.1.3",
    "moment": "2.17.1",
    "mongodb": "2.2.19",
    "node-sass": "4.1.1",
    "raspar": "1.1.0",
    "request": "2.79.0",
    "universal-analytics": "0.4.8"
  },
  "scripts": {
    "start": "node web.js",
    "build": "npm run build:css",
    "build:css": "node-sass --output-style compressed --output static/css static/css/styles.scss",
    "watch:css": "node-sass --output-style compressed --watch --recursive --output static/css static/css/styles.scss",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/neogeek/doxdox-server.git"
  },
  "private": true
}
